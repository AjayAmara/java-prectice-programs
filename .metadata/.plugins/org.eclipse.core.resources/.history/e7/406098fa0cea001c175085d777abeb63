package exam;

public class Students {
	
	public static void main(String[] args) {
		int students=4;
		int[] totalMarks= {12,23,32,34};
		int sortedStudents=2;
		
		for(int i=0;i<students-1;i++) {
			
			for(int j=0; j<totalMarks.length;i++) {
				
			if(totalMarks[i]<totalMarks[i+1]) {
				
				int rep=totalMarks[i];
				totalMarks[i]=totalMarks[i+1];
				totalMarks[i+1]=rep;
				
			}
				}
			}
		for(int i=0;i<=totalMarks.length-1;i++) {
			
			System.out.println(totalMarks[i]);
		}
		
			
		
		
	}

}
